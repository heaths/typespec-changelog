{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "tsp compile",
            "skipFiles": [
                "node_modules/**"
            ],
            "runtimeExecutable": "npm",
            "runtimeArgs": [
              "run",
              "test:compile"
            ],
            "preLaunchTask": "npm: build",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ]
        }
    ]
}
